{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test-module/**/*.ts\" \"test-unit/**/*.ts\"  \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:dev:eventstorage-inmemory": "cross-env EVENTSOURCING_MODE=inmemory nest start --watch",
    "start:dev:eventstorage-inmemory:debug": "cross-env EVENTSOURCING_MODE=inmemory nest start --debug '0.0.0.0:9229' --watch",
    "start:dev:eventstorage-typeorm:debug": "cross-env EVENTSOURCING_MODE=typeorm nest start --debug '0.0.0.0:9229' --watch",
    "start:dev:eventstorage-typeorm:docker": "scripts/start_dev_eventstorage-typeorm_docker.sh",
    "start:dev:eventstorage-typeorm:debug:docker": "scripts/start_dev_eventstorage-typeorm_debug_docker.sh",
    "start:dev:eventstorage-eventstore:docker": "scripts/start_dev_eventstorage-eventstore_docker.sh",
    "start:dev:eventstorage-eventstore:debug": "cross-env EVENTSOURCING_MODE=eventstore EVENTSTORE_URL=http://127.0.0.1:2113 EVENTSTORE_USERNAME=admin EVENTSTORE_PASSWORD=changeit nest start --debug '0.0.0.0:9229' --watch",
    "start:dev:eventstorage-eventstore:debug+docker": "scripts/start_dev_eventstorage-eventstore_debug_docker.sh",
    "start:debug": "nest start --debug '0.0.0.0:9229' --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test-e2e,test-module,test-unit}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:all": "jest",
    "test:all:watch": "jest --watch",
    "test:all:cov": "jest --coverage",
    "test:unit": "jest --config test-unit/jest-unit.json",
    "test:unit:watch": "jest --watch --config test-unit/jest-unit.json",
    "test:unit:cov": "jest --coverage --config test-unit/jest-unit.json",
    "test:integration": "jest --config test-integration/jest-integration.json",
    "test:integration:cov": "jest --coverage --config test-integration/jest-integration.json",
    "test:integration:docker": "scripts/test_integration_docker.sh",
    "test:module": "jest --config test-module/jest-module.json",
    "test:module:cov": "jest --coverage --config test-module/jest-module.json",
    "test:debug": "node --inspect-brk=0.0.0.0:9229 -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config test-e2e/jest-e2e.json --passWithNoTests",
    "ci-format": "prettier -c \"src/**/*.ts\" \"test-module/**/*.ts\" \"test-unit/**/*.ts\"  \"libs/**/*.ts\"",
    "ci-lint": "eslint \"{src,apps,libs,test-e2e,test-module,test-unit}/**/*.ts\" --format codeframe"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.11",
    "@nestjs/core": "^7.0.11",
    "@nestjs/cqrs": "^7.0.0",
    "@nestjs/platform-express": "^7.0.11",
    "@nestjs/swagger": "^4.5.6",
    "@nestjs/typeorm": "^7.0.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "moment": "^2.25.3",
    "pg": "^8.2.0",
    "postgres": "^1.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.1.5",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.11",
    "@types/express": "^4.17.6",
    "@types/jest": "25.2.2",
    "@types/node": "^14.0.1",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "jest": "25.5.4",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "25.5.1",
    "ts-loader": "^7.0.4",
    "ts-node": "^8.10.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/test-e2e/",
      "<rootDir>/test-module/",
      "<rootDir>/test-unit/",
      "<rootDir>/test-integration/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@coders-board-library/event-sourcing/(.*)": "<rootDir>/libs/event-sourcing/src/$1",
      "@coders-board-library/event-sourcing": "<rootDir>/libs/event-sourcing/src",
      "@coders-board-library/time-provider/(.*)": "<rootDir>/libs/time-provider/src/$1",
      "@coders-board-library/time-provider": "<rootDir>/libs/time-provider/src",
      "@coders-board-library/public-messages/(.*)": "<rootDir>/libs/public-messages/src/$1",
      "@coders-board-library/public-messages": "<rootDir>/libs/public-messages/src",
      "@coders-board-library/rxjs-utils/(.*)": "<rootDir>/libs/rxjs-utils/src/$1",
      "@coders-board-library/rxjs-utils": "<rootDir>/libs/rxjs-utils/src",
      "@coders-board-library/axios-utils/(.*)": "<rootDir>/libs/axios-utils/src/$1",
      "@coders-board-library/axios-utils": "<rootDir>/libs/axios-utils/src",
      "@coders-board-library/eventstore-projections/(.*)": "<rootDir>/libs/eventstore-projections/src/$1",
      "@coders-board-library/eventstore-projections": "<rootDir>/libs/eventstore-projections/src"
    }
  },
  "eslintIgnore": [
    "**/*.projection.*"
  ]
}
